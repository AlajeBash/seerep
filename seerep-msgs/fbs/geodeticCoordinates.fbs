namespace seerep.fb;

// source: https://en.wikipedia.org/wiki/Geodetic_coordinates

table geodeticCoordinates {
    coordinateSystem:string; // e.g EPSG::4326

    ellipsoid:string; // e.g EPSG::7030

    longitute:int; // longitude measured in degrees
    latitude:int; // latitude measured in degrees
    altitude:int; // ellipsoidal height measured as a distance
}
