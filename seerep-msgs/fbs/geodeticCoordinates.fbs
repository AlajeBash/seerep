namespace seerep.fb;

// source: https://en.wikipedia.org/wiki/Geodetic_coordinates

table geodeticCoordinates {
    coordinateSystem:string; // e.g EPSG::4326

    ellipsoid:string; // e.g EPSG::7030

    longitute:double; // longitude measured in degrees
    latitude:double; // latitude measured in degrees
    altitude:double; // ellipsoidal height measured as a distance
}
