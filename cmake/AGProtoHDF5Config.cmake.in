@PACKAGE_INIT@

list(APPEND CMAKE_MODULE_PATH "${CURRENT_LIST_DIR}/Modules")

set(AGProtoHDF5_INCLUDE_DIRS @PACKAGE_CMAKE_INSTALL_INCLUDEDIR@)

find_package(PkgConfig REQUIRED)
find_library(AGProtoHDF5_LIBRARY NAMES agprotohdf5)
set(AGProtoHDF5_LIBRARIES ${AGProtoHDF5_LIBRARY})

find_package(Protobuf REQUIRED)
list(APPEND AGProtoHDF5_INCLUDE_DIRS ${Protobuf_INCLUDE_DIR})
list(APPEND AGProtoHDF5_LIBRARIES ${Protobuf_LIBRARY})

find_package(HDF5 REQUIRED)
list(APPEND AGProtoHDF5_INCLUDE_DIRS ${HDF5_INCLUDE_DIRS})
list(APPEND AGProtoHDF5_LIBRARIES ${HDF5_LIBRARIES})

find_package(HighFive REQUIRED)
list(APPEND AGProtoHDF5_INCLUDE_DIRS ${HighFive_INCLUDE_DIRS})
list(APPEND AGProtoHDF5_LIBRARIES ${HighFive_LIBRARIES})
